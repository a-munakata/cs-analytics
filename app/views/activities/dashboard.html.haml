= content_for :body do
  .result_container
    %section
      %h1.user
        .ir.user
        Users
      %ul
        %li="総ユーザー数 : #{@users_num}"
        %li="アクティブユーザー/1日 : #{@dau}"
        %li="アクティブユーザー/1週 : #{@wau}"
        %li="アクティブユーザー/1月 : #{@mau}"
