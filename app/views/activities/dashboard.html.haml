      %ul
        %li="総サーベイ数 : #{@surveys_num}"
        %li="平均サーベイ数 / ユーザー : #{@surveys_num/@users_num}"
    %section
      %h1.customer
        .ir.customer
        Customers
      %ul
        - @customers_hash.each do |cus|
          %li="#{cus.keys.first} : #{cus.values.first}"

